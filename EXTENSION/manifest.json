{
    "manifest_version": 2,

    "name": "Phisherman",
    "description": "Welcome to Phisherman! This extension will help you identify whether a website is phishing or not. ",
    "version": "1.0",

    "browser_action": {
     "default_icon": "images/icon.png",
     "default_popup": "popup.html"
    },
     "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "permissions": [
      "activeTab",
      "tabs",
      "https://ajax.googleapis.com/",
      "tabs",
      "<all_urls>"
    ]
  }
